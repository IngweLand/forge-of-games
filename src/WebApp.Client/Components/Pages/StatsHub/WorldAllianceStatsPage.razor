@attribute [Route(FogUrlBuilder.PageRoutes.WORLD_ALLIANCES_TEMPLATE)]
@using Ingweland.Fog.Application.Core.Helpers
@using Ingweland.Fog.WebApp.Client.Components.Elements.StatsHub
@inherits WorldStatsPageBase<Ingweland.Fog.Application.Client.Web.StatsHub.ViewModels.AllianceViewModel>

@if (IsInitialized)
{
    <PaginatedStatsListComponent Title="@Title" OnSearch="Search" ItemsProviderDelegate="GetDataAsync"
                                 NameSearchString="@NameSearchString">
        <ItemTemplate >
            <AllianceListItemComponent Alliance="@context" OnClick="OnItemClicked" />
        </ItemTemplate>
    </PaginatedStatsListComponent>
}
else
{
    <LoadingIndicator/>
}

