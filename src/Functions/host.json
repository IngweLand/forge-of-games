{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "functionTimeout": "00:10:00",
    "extensions": {
        "queues": {
            "maxDequeueCount": 1
        },
        "durableTask": {
            "maxConcurrentActivityFunctions": 1,
            "maxConcurrentOrchestratorFunctions": 1
        }
    }
}